@model IEnumerable<EFCodeFirst.Domain.Product>

@{
    ViewBag.Title = "Products";
}

<h2>Products</h2>

<table>
    <tr>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        
    </tr>
}

</table>
